    "echarts": "^5.3.3",
    "github-markdown-css": "^5.8.1",
    "lodash": "^4.17.21",
    "mobx": "^6.13.0",
    "mobx-react-lite": "^4.0.7",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
